{
    "editor.insertSpaces": false,
    "eslint.autoFixOnSave": true,
    "editor.tabSize": 4,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*.css": true
    },
    "path-intellisense.mappings": {
        "utils": "${workspaceRoot}/src/utils",
        "assets": "${workspaceRoot}/src/assets"
    }
}